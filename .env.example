# Google Custom Search API credentials
SEARCH_GOOGLE_API_KEY=your_google_api_key_here
SEARCH_GOOGLE_CX=your_google_search_engine_id_here

# Bing Search API credentials
SEARCH_BING_API_KEY=your_bing_api_key_here

# API Settings
SEARCH_API_HOST=0.0.0.0
SEARCH_API_PORT=8000
SEARCH_LOG_LEVEL=INFO

# Rate limiting
SEARCH_ENABLE_RATE_LIMIT=true
SEARCH_RATE_LIMIT=100

# Cache settings
SEARCH_ENABLE_CACHE=true
SEARCH_CACHE_TTL=3600